{
  "name": "rewiremock",
  "version": "2.0.0",
  "description": "Easy and es6 compatible dependency mocking tool.",
  "main": "lib/index.js",
  "module": "src/index.js",
  "types": "rewiremock.d.ts",
  "scripts": {
    "test": "npm run test:pick -- '_tests/**/*spec.js'",
    "test:pick": "BABEL_ENV=test mocha --compilers js:babel-core/register",
    "test:karma": "node ./node_modules/karma/bin/karma start --single-run --browsers PhantomJS",
    "build": "babel src -d lib",
    "prepublish": "npm run build",
    "lint": "eslint src tests",
    "lint:fix": "eslint src tests --fix"
  },
  "keywords": [
    "es6",
    "require",
    "dependency",
    "injection",
    "dependency injection",
    "mock dependency",
    "di",
    "inject",
    "swap",
    "test",
    "mock",
    "stub"
  ],
  "author": "Anton Korzunov (thekashey@gmail.com)",
  "license": "MIT",
  "dependencies": {
    "lodash.some": "^4.6.0",
    "lodash.template": "^4.4.0",
    "wipe-node-cache": "^1.0.2",
    "wipe-webpack-cache": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-dynamic-import-node": "^1.0.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-latest": "^6.16.0",
    "babel-register": "6.18.0",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-plugin-mocha": "^4.9.0",
    "mocha": "^3.0.2",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^2.0.4",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-polyfill-object-assign": "0.0.2",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  }
}
